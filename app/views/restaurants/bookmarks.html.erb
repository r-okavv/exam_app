<div class="container mx-auto">
  <h1 class="text-2xl mt-4 mb-4">ブックマーク一覧</h1>

  <% if @bookmark_restaurants.present? %>
      <%= render @bookmark_restaurants %>
    <div class="mt-4 mb-6 text-center">
      <%= paginate @bookmark_restaurants %>
    </div>
  <% else %>
    <p>Bookmarkされた店舗はありません</p>
  <% end %>
</div>
